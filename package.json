{
  "name": "jquery-picknick",
  "version": "1.0.1",
  "description": "Helps select things",
  "keywords": [
    "gallery",
    "menu",
    "slides",
    "tabs"
  ],
  "main": "jquery.picknick.js",
  "homepage": "https://github.com/thewhodidthis/jquery-picknick#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/jquery-picknick.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/jquery-picknick/issues"
  },
  "directories": {
    "example": "example"
  },
  "config": {
    "remote": "thewhodidthis@zazie:www/example/jquery-picknick/"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint jquery.picknick.js",
    "start": "uglifyjs jquery.picknick.js -cmo jquery.picknick.min.js",
    "example": "npm run example:build && npm run example:serve",
    "example:build": "ln -siv \"$(pwd)/jquery.picknick.js\" example",
    "example:serve": "php -S localhost:8000 -t example/",
    "example:press": "rsync -avruzLPC --delete --exclude=.DS_Store ./example/ $npm_package_config_remote"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-jquery": "latest",
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense"
}
