{
  "name": "jquery-picknick",
  "version": "1.0.1",
  "description": "Helps select things",
  "keywords": [
    "gallery",
    "menu",
    "slides",
    "tabs"
  ],
  "main": "jquery.picknick.js",
  "homepage": "https://github.com/thewhodidthis/jquery-picknick#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thewhodidthis/jquery-picknick.git"
  },
  "bugs": {
    "url": "https://github.com/thewhodidthis/jquery-picknick/issues"
  },
  "directories": {
    "example": "example"
  },
  "config": {
    "jscsrc": ".jscsrc",
    "remote": "thewhodidthis@zazie:www/example/jquery-picknick/"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "npm run clear:dist && npm run clear:logs",
    "clear:dist": "rm -rf dist/",
    "clear:logs": "rm -f *.log",
    "lint": "(jshint jquery.picknick.js & jscs jquery.picknick.js --reporter=inlinesingle) 2>&1 | tee lint-js.log",
    "postlint": "find lint-js.log -size 0 -print0 | xargs -0 rm",
    "start": "npm run build",
    "preexample": "npm run build",
    "example": "npm run example:link && npm run example:serve",
    "example:link": "ln -siv \"$(pwd)/jquery.picknick.js\" example",
    "example:push": "rsync -avruzLPC --exclude=.DS_Store ./example/ $npm_package_config_remote",
    "example:serve": "php -S localhost:8000 -t example/",
    "build": "uglifyjs jquery.picknick.js -cm > jquery.picknick.min.js",
    "preversion": "npm run clear && npm run build"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.5.3"
  },
  "dependencies": {},
  "devDependencies": {
    "uglify-js": "latest"
  },
  "author": "sChzs BkgNns <sotirios.bakagiannis@gmail.com> (https://www.thewhodidthis.com)",
  "license": "Unlicense",
  "private": true
}
